{
  "buildCommand": "cd ../packages/core && pnpm build && cd ../methods && pnpm build && cd ../../frameworks/qwik && pnpm build && cd ../../website && pnpm sitemap && pnpm llms && pnpm build",
  "headers": [
    {
      "source": "/(.*)?service-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(assets|build)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    }
  ]
}
